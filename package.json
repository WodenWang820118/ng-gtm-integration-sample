{
  "name": "ng-gtm-integration-sample",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "on-emulator": "ng serve --disable-host-check --host 0.0.0.0",
    "backend": "json-server --watch db.json",
    "build": "ng build --configuration production --base-href https://wodenwang820118.github.io/ng-gtm-integration-sample/browser --optimization --output-path=dist/ng-gtm-integration-sample",
    "build-file": "ng build --configuration production --base-href ./",
    "deploy": "ngh --dir=dist/ng-gtm-integration-sample/browser",
    "build-github": "npm run build && npm run deploy",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "bundle-report": "ng build --configuration production --source-map && source-map-explorer dist/ng-gtm-integration-sample/browser/*.js"
  },
  "private": true,
  "dependencies": {
    "@amcharts/amcharts5": "^5.9.12",
    "@amcharts/amcharts5-fonts": "^5.0.1",
    "@amcharts/amcharts5-geodata": "^5.1.3",
    "@angular/animations": "^17.3.0",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "@angular/service-worker": "^17.3.0",
    "@angular/youtube-player": "^17.0.3",
    "@fortawesome/angular-fontawesome": "^0.14.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@ng-bootstrap/ng-bootstrap": "^16.0.0",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.1",
    "dexie": "^3.2.4",
    "firebase": "^10.12.2",
    "fuse.js": "^7.0.0",
    "json-server": "^0.17.4",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "^9.0.1",
    "world-countries": "^5.0.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.6",
    "@angular/cli": "^17.3.0",
    "@angular/compiler-cli": "^17.3.0",
    "@types/jasmine": "~4.3.0",
    "@types/uuid": "^9.0.7",
    "angular-cli-ghpages": "^0.6.2",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "shx": "^0.3.4",
    "source-map-explorer": "^2.5.3",
    "typescript": "~5.2.2"
  }
}
